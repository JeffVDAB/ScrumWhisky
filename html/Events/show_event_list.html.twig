{% extends 'base.html.twig' %}

{% block title %}
  List of events
{% endblock title %}

{% block content %}
  <div class="container form">
    <h1>List of events</h1>
    <div>
	    <dl>
	        {% for event in events %}
	        <dt>{{ event.Title }}</dt> 
	        	<dd>{{ event.EventDate|date('d-m-Y') }}</dd>        
	        {% endfor %}  
	     </dl>
	</div>

    <a href="/" id="return_to_main" class="btn submit btn-default">
          Return to main menu</a>    
  </div>
{% endblock content %}